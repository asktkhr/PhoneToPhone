<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:id="@+id/registerButton" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:text="register"
		android:onClick="registerDevice"></Button>
	<Button android:id="@+id/unregisterButton" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:text="unregister"
		android:onClick="unregisterDevice"></Button>
</LinearLayout>
